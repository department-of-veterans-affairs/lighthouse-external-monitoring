{
  "name": "$name",
  "type": "http",
  "host": "$host",
  "port": 443,
  "url": "$url",
  "requestheaderContentType": "Content-Type:application/x-www-form-urlencoded",
  "requestheaderAuthorization": "Authorization:Basic $authorization_token",
  "postdata": "grant_type=refresh_token&refresh_token=$refresh_token",
  "encryption": true,
  "verify_certificate": true,
  "resolution": 1,
  "responsetime_threshold": 30000,
  "sendnotificationwhendown": 6,
  "notifyagainevery": 0,
  "notifywhenbackup": true,
  "tags": [ "$build", "managed", "oauth" ],
  "integrationids": [ $integrationids_csv ],
  "probe_filters": [ "region: NA" ]
}
