{
  "name": "$name",
  "type": "http",
  "host": "$host",
  "port": 443,
  "url": "$url",
  "requestheaderAuthorization":"Authorization:Bearer $authorization_token",
  "encryption": true,
  "verify_certificate": true,
  "resolution": 1,
  "responsetime_threshold": 30000,
  "sendnotificationwhendown": 0,
  "notifyagainevery": 2,
  "notifywhenbackup": true,
  "tags": [ "$build", "managed", "$group" ],
  "integrationids": [ $integrationids_csv ],
  "probe_filters": [ "region: NA" ]
}
